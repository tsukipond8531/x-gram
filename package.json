{
  "name": "ProjectName",
  "version": "0.0.1",
  "private": true,
  "codepush": {
    "ios_app_id": "vucms/XGram-iOS",
    "android_app_id": "vucms/XGram-Android"
  },
  "scripts": {
    "android": "react-native run-android",
    "android:release": "react-native run-android --variant=release",
    "ios": "react-native run-ios",
    "start": "npx react-native start --reset-cache",
    "test": "jest",
    "lint": "eslint .",
    "clean:android": "cd android && ./gradlew clean && cd ../",
    "build:android": "cd android && ./gradlew assembleRelease",
    "clean:ios": "cd ios && xcodebuild clean && cd ../",
    "pod": "npx pod-install",
    "ip": "yarn install && yarn pod && yarn ios",
    "f:ios": "cd ios && fastlane ioslane",
    "f:android": "cd android && fastlane androidlane",
    "clean": "rm -rf node_modules && yarn cache clean && yarn install && cd ios && pod install && cd ..",
    "c:ios": "appcenter codepush release-react -a $npm_package_codepush_ios_app_id -d Staging --plist-file ios/$npm_package_name/Info.plist --mandatory true",
    "c:android": "appcenter codepush release-react -a $npm_package_codepush_android_app_id -d Staging --mandatory true",
    "postinstall": "npx patch-package"
  },
  "dependencies": {
    "@gorhom/bottom-sheet": "^4",
    "@gorhom/portal": "^1.0.14",
    "@react-native-community/blur": "^4.2.0",
